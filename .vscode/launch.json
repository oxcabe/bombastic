{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bun",
      "request": "launch",
      "name": "Debug File",
      "runtime": "/home/oxcabe/devel/toc/bombastic/.devbox/nix/profile/default/bin/bun",
      "program": "${file}",
